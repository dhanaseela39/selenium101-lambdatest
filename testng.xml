<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="LambdaTest Suite" parallel="tests" thread-count="2">
    <test name="Chrome Test">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <<class name="tests.InputFormTest"/>
            <class name="tests.SimpleFormTest"/>
            <class name="tests.SliderDemoTest"/>
        </classes>
    </test>

    <test name="Safari Test">
        <parameter name="browser" value="safari"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="macOS Catalina"/>
        <classes>
            <class name="tests.InputFormTest"/>
            <class name="tests.SimpleFormTest"/>
            <class name="tests.SliderDemoTest"/>
        </classes>
    </test>
</suite>
